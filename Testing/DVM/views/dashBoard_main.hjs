<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Movement Visualisation - 4th Year Project</title>

    <!-- Bootstrap Core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="css/liveStat.css" rel="stylesheet">
    <link href="css/sb-admin.css" rel="stylesheet">
    <link href="css/heatmap.css" rel="stylesheet">
    <link href="css/livestream.css" rel="stylesheet">



    <!-- Morris Charts CSS -->
    <link href="css/plugins/morris.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="main">Pedestrian Movement Visualisation</a>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                <li class="dropdown">
                    <!--These alerts should be populated using jQuery-->
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-bell"></i> <b class="caret"></b></a>
                    <ul class="dropdown-menu alert-dropdown">
                        <li>
                            <a href="#">{Username;Timestamp}<span class="label label-default">Perimeter exit</span></a>
                        </li>
                        <li>
                            <a href="#">{Username;Timestamp}<span class="label label-primary">New perimeter entry</span></a>
                        </li>
                        <li>
                            <a href="#">{Username;Timestamp}<span class="label label-success">Authorised area entry</span></a>
                        </li>
                        <li>
                            <a href="#">{Username;Timestamp}<span class="label label-warning">Prolonged presence</span></a>
                        </li>
                        <li>
                            <a href="#">{Username;Timestamp}<span class="label label-danger">Area violation</span></a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#">View All</a>
                        </li>
                    </ul>
                </li>
                <li class="dropdown">
                    <!--This is where the templating engine inserts the name of the individuals who has logged in-->
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i>{{user}}<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="#"><i class="fa fa-fw fa-user"></i> Profile</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-fw fa-gear"></i> Settings</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#"><i class="fa fa-fw fa-power-off"></i> Log Out</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active">
                        <a href="main"><i class="fa fa-fw fa-dashboard"></i> Real-time Stream</a>
                    </li>
                    <li>
                        <a href="javascript:;" data-toggle="collapse" data-target="#demo"><i class="fa fa-fw fa-bar-chart-o"></i> Analysis <i class="fa fa-fw fa-caret-down"></i></a>
                        <ul id="demo" class="collapse">
                            <li>
                                <a href="historicalHeatmap">Historical Heatmap</a>
                            </li>
                            <li>
                                <a href="presenceLaneChart">Presence Lane Chart</a>
                            </li>
                            <li>
                                <a href="stringGraph">String graph</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-fw fa-edit"></i>Report Generation</a>
                    </li>
            </div>
            <!-- /.navbar-collapse -->
        </nav>

        <div id="page-wrapper">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Live Stream <small>Real-time Heatmap of Area Movement Activity </small>
                        </h1>
                    </div>
                </div>
                <!-- /.row -->

                <!-- Alerts-->
                <!-- These should be populated using jQuery-->
                <div class="row">
                    <div class="col-lg-12" id="areaAlertMessage">
                        <div class="alert alert-danger alert-dismissable">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                            <i class="fa fa-info-circle"></i>  <strong>Warning!</strong> Unauthorized area entry
                        </div>
                    </div>
                </div>
                <!-- /.row -->

                <!-- Heatmap Visualisation -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-info" id=liveHeatmapPanel>
                            <div class="panel-heading">
                                <h3 class="panel-title"><i class="fa fa-dashboard fa-fw"></i> Surveilance area</h3>
                            </div>
                            <div class="panel-body">
                                <div id="heatmapContainerWrapper"><div id="heatmapContainer"></div></div>
                                <div class="HeatTooltip1"></div>
                                <div class="HeatTooltip2"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.row -->

                <!-- Supporting Vis -->
                <div class="row">
                    <div class="col-lg-8">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <h3 class="panel-title"><i class="fa fa-dashboard fa-fw"></i> Velocity Stream Graph</h3>
                            </div>
                            <div class="panel-body">
                                <div id="liveSpeedStream"></div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <h3 class="panel-title"><i class="fa fa-clock-o fa-fw"></i> Live Statistics</h3>
                            </div>
                            <div class="table-responsive">
                              <table class="table table-bordered table-hover table-striped">
                                  <thead>
                                      <tr>
                                          <th>Device Name</th>
                                          <th>ID Color</th>
                                          <th>Time in Area (sec)</th>
                                          <th>Entry frequency</th>
                                      </tr>
                                  </thead>
                                  <tbody>
                                      <tr class="active" id="liveStat1">
                                          <td>iPhone</td>
                                          <td>
                                            <div class="progress">
                                              <div class="progress-bar progress-bar-dev1" role="progressbar" style="width: 100%;">
                                            </div>
                                          </td>
                                          <td id="dev1Time">
                                            <label id="minutes1">00</label>:<label id="seconds1">00</label>
                                          </td>
                                          <td id="dev1Feq">0</td>
                                      </tr>
                                      <tr class="active" id="liveStat2">
                                          <td>iPad</td>
                                          <td>
                                            <div class="progress">
                                              <div class="progress-bar progress-bar-dev2" role="progressbar" style="width: 100%;">
                                            </div>
                                          </td>
                                          <td id="dev2Time">
                                            <label id="minutes2">00</label>:<label id="seconds2">00</label>
                                          </td>
                                          <td id="dev2Feq">0</td>
                                      </tr>
                                  </tbody>
                              </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.row -->

            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- On page content -->
    <audio src="audio/siren.wav" id="AlertTone" autostart="false"></audio>

    <!--  ****SCRIPTS***  -->

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Socket.js Javascript -->
    <script src="js/socket.io.js"></script>

    <!-- D3.js Javascript -->
    <script src="js/d3.js"></script>

    <!--  Heatmap Javascript -->
    <script src='js/heatmap.js'></script>
    <script src='js/liveHeatmap.js'></script>
    <script src='js/liveSpeedStream.js'></script>
    <script src='js/liveStats.js'></script>


    <!--  Live Updates -->
    <script src='js/liveAlert.js'></script>

</body>

</html>
